<%= form_for([@todo_list, @todo_list.todo_items.build]) do |f| %>
 <%= f.text_field :notes, placeholder: "Add details about your build such as coordinates etc..." %>
 <%= f.select :blocks, options_for_select(MinecraftItems.all_variants), {multiple: true, include_blank: true}, {class: 'ui fluid search dropdown'} %>
 <%= f.submit %>
<% end %>